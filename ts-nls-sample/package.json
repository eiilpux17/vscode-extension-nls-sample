{
	"name": "ts-nls-sample",
	"displayName": "typescript-nls-sample",
	"description": "%tsnls.sample.description%",
	"version": "0.0.1",
	"publisher": "vscode-samples",
	"repository": "https://github.com/Microsoft/vscode-extension-samples/helloworld-sample",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"compile": "tsc -p",
		"watch": "tsc -b -w"
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.34.0",
		"@typescript-eslint/eslint-plugin": "^4.16.0",
		"@typescript-eslint/parser": "^4.16.0",
		"del": "^6.0.0",
		"eslint": "^7.21.0",
		"gulp": "^4.0.2",
		"gulp-rename": "^2.0.0",
		"gulp-sourcemaps": "^3.0.0",
		"gulp-typescript": "^6.0.0-alpha.1",
		"typescript": "^4.2.2",
		"vscode-nls-dev": "^3.3.2"
	},
	"dependencies": {
		"vscode-nls": "^5.0.0"
	}
}
